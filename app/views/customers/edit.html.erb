<% if flash[:error] %>
  <li class="alert alert-error"><%= flash[:error]  %></li>
  <% end %>
<% if notice %>
  <li class="alert alert-success"><%= notice  %></li>
  <% end %>

<div class="section_complete">
  <h3><%= t('customers.editing.title') %></h3>
  <div class="data">
      <%= render 'form' %>
  </div>
<%= link_to t('labels.show') , @customer %> |
<%= link_to t('labels.back'), customers_path %>
</div>


