<h1>Listing tickets all</h1>
  <div class="span3">
      <%= render "view" %>
  </div>
<%= current_user.role_id %>
<div class="span10">
  <h4> Incidencias sin resolver (<%= @tickets_all.count  %>)</h4>
  <table class="table">
      <th>#</th>
      <th>Issue</th>
      <th>Customer</th>
      <th>Created at</th>
      <th>priority</th>
    <% @tickets_all.each do |ticket| %>       
      <tr>
        <td><%= ticket.id %></td>
        <td><li class="<%= ticket.prioritycss %>">  </li></td>
        <td><%= link_to ticket.customer.lastname + ", "+ ticket.customer.name, customer_reply_path(ticket.customer.id, ticket.id) %></td>
        <td><%= ticket.created_at %></td>
        <td><%= ticket.priority %></td>
    <tr>
    <% end %>
  </table>
  </div>

